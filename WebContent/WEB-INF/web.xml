<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

	<!--  General Web Application Description -->
	<display-name>Multiplier built as JSP</display-name>

	
 	 <welcome-file-list>
 	     <welcome-file>
 	         menu.html
 	     </welcome-file>
 	</welcome-file-list>
 	
	<description>
		Multiplies times 3, using JSP
 	</description>
 	
 	<servlet>
 		<servlet-name>login</servlet-name>
 		<servlet-class>cse135.servlet.LoginServlet</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>login</servlet-name>
 		<url-pattern>/loginAttempt</url-pattern>
 	</servlet-mapping>
 	
 	<servlet>
 		<servlet-name>signup</servlet-name>
 		<servlet-class>cse135.servlet.SignupServlet</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>signup</servlet-name>
 		<url-pattern>/signupAttempt</url-pattern>
 	</servlet-mapping>
 	
 	<servlet>
 		<servlet-name>category</servlet-name>
 		<jsp-file>/categories.jsp</jsp-file>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>category</servlet-name>
 		<url-pattern>/categories</url-pattern>
 	</servlet-mapping>
 	
	<servlet>
		<servlet-name>multiplier</servlet-name>
		<jsp-file>/multiplyJSP.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>multiplier</servlet-name>
		<url-pattern>/multiply</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>student</servlet-name>
		<jsp-file>/students.jsp</jsp-file>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>student</servlet-name>
		<url-pattern>/students</url-pattern>
	</servlet-mapping>

</web-app>
